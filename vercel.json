{
  "version": 2,
  "builds": [
    {
      "src": "frontend/angular.json",
      "use": "@vercel/static-build",
      "config": {}
    },
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.ts" },
    { "src": "/(.*)", "dest": "/frontend/dist/frontend/index.html" }
  ]
}
